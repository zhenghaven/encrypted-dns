{
    "version": "1.2.0",
    "ecs_ip_address": null,
    "dnssec": false,
    "dns_cache": {
        "enable": true,
        "override_ttl": 3600
    },
    "firewall": {
        "refuse_ANY": true,
        "disable_AAAA": false,
        "rate_limit": 60000,
        "client_blacklist": []
    },
    "rules": {
        "force_safe_search": false,
        "hosts": {
            "localhost"          : "127.0.0.1",
            "cloudflare-dns.com" : "1.1.1.1",
            "dns.google"         : "8.8.8.8",
            "dns.google.com"     : "8.8.8.8",
            "dns.alidns.com"     : "223.5.5.5",
            "dns2.alidns.com"    : "223.6.6.6"
        }
    },
    "inbounds": [
        "0.0.0.0:53",
        "tcp://0.0.0.0:5301"
    ],
    "outbounds": [
        {
            "tag": "bootstrap",
            "dns": [
                "0.0.0.0"
            ],
            "domains": []
        },
        {
            "tag": "default",
            "dns": [
                "tls://cloudflare-dns.com",
                "tls://1.0.0.1",
                "tls://dns.google",
                "tls://8.8.4.4"
            ],
            "concurrent": false,
            "domains": [
                "all"
            ]
        },
        {
            "tag": "encrypted",
            "dns": [
                "tls://cloudflare-dns.com",
                "tls://1.0.0.1",
                "tls://dns.google",
                "tls://8.8.4.4"
            ],
            "concurrent": false,
            "domains": [
                "sub:google.com",
                "sub:gmail.com",
                "sub:facebook.com"
            ],
            "notify": [
                50001
            ]
        }
    ]
}
